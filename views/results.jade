block misc
	script(type='text/javascript')
		$(function() {
				$( "#score-range" ).slider({
					range: true,
					min: 0,
					max: 100,
					values: [ 0, 100 ],
					slide: function( event, ui ) {
						$( "#score" ).val( ui.values[ 0 ] + " - " + ui.values[ 1 ] );
					}
				});
				$( "#score" ).val( $( "#score-range" ).slider( "values", 0 ) +
					" - " + $( "#score-range" ).slider( "values", 1 ) );
			});